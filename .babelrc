{
  "env": {
    "test": {
      "presets": ["es2015", "stage-0", "react"],
      "plugins": [
        "rewire",
        ["module-resolver", {
          "root": ["./src", "./test"],
          "alias": {
            "src": "./src",
            "test": "./test",
            "assets": "./src/assets",
            "modules": "./src/modules",
            "utils": "./src/utils",
            "services": "./src/services"
          },
          "extensions": [".js", ".jsx"]
        }]
      ]
    }
  },
  "presets": [["es2015", {"modules": false}], "stage-0", "react"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "react-hot-loader/babel"
  ],
  "compact": false
}
